{
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "require-jsdoc": "off",
    "max-len": [2, 120, 8]
  },
  "env": {
    "mocha": true,
    "amd": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
